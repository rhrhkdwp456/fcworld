<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link href="/main.css" rel="stylesheet">
</head>
<body>

    <%- include('nav_login.ejs') %>
    
    <body class="grey-bg">
        <div class="white-bg">
            <div class="content">
                <h2 style="font-weight: bold; padding-top: 30px;">FC Stagram</h2>
                <br>
                <div class="content-parent">
                    <% for(let i = 0;i< result.length ;i++){ %>
                        <div class="content-box">
                            <h4 style="font-weight: bold;"><% result[i].title %></h4>
                            <br>
                            <a href="/detail/<%= result[i]._id %>" style="text-decoration: none"><img src="<%= result[i].img %>" ></a>
                            <hr style="margin-top: 60px">
                        </div>
                    <% } %>
                </div>
            </div>
            <div class="parent" style="width: 100%; height: 600px; background: #585858;">
                <div class="child" style="background: #2E9AFE;">
            
                </div>
                <div class="child" style="background: #FA5858;">
            
                </div>
                <div class="child" style="background: #81F781;">
            
                </div>
            </div>
            
        </div> 
    <script>
        document.querySelectorAll('.content-box')[0].addEventListener('click', function(e){

        })
    </script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>
</html>